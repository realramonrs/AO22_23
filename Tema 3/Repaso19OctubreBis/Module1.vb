Module Module1

    Sub Main()
        'Programa que indica si un número es múltiplo de 2 , si es múltiplo de 3, o si es múltiplo de 7
        'En caso de que no sea múltiplo de ninguno indica con un mensaje dicha situación
        Dim numero As Integer = 20

        If numero Mod 2 = 0 Then
            Console.WriteLine("múltiplo de 2")

        End If
    End Sub

End Module
